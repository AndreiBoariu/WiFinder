pod 'SwiftyJSON'
pod 'GrowingTextView'

  target 'WiFinderTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'WiFinderUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
